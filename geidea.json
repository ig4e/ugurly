      "put": {
        "summary": "Update Payment Intent",
        "description": "API to update the Payment Intent",
        "operationId": "update-payment-intent",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "paymentIntentId",
                  "amount",
                  "currency"
                ],
                "properties": {
                  "paymentIntentId": {
                    "type": "string",
                    "description": "Payment Intent ID generated when a new request was created.",
                    "default": "236b4763-0bc5-4d8b-f2ef-08db4614f0a2"
                  },
                  "amount": {
                    "type": "number",
                    "description": "Amount for which payment link is created. Amount must be greater than 0.01. Must be equal to [eInvoiceItems.price * eInvoiceItems.quantity]",
                    "default": 20,
                    "format": "double"
                  },
                  "currency": {
                    "type": "string",
                    "description": "Currency for which payment link is created. Currency must be 3 characters in length. Acceptable currencies are: SAR, AED",
                    "enum": [
                      "EGP",
                      "SAR"
                    ]
                  },
                  "customer": {
                    "type": "object",
                    "description": "Customer Object",
                    "required": [
                      "name",
                      "email",
                      "phoneCountryCode",
                      "phoneNumber"
                    ],
                    "properties": {
                      "name": {
                        "type": "string",
                        "description": "Customer Name",
                        "default": "Kiran"
                      },
                      "email": {
                        "type": "string",
                        "description": "Email of customer. One of email or phoneNumber must be present.",
                        "default": "testmail@geidea.com"
                      },
                      "phoneCountryCode": {
                        "type": "string",
                        "description": "Country code of customer's phone number eg: \"+971\". If phoneNumber is passed, phoneCountryCode must be passed.",
                        "default": "+20"
                      },
                      "phoneNumber": {
                        "type": "string",
                        "description": "Customer's phone number  eg: \"511111111\". If phoneCountryCode is passed, phoneNumber must be passed.  One of email or phoneNumber must be present.",
                        "default": "8003030083"
                      }
                    }
                  },
                  "eInvoiceDetails": {
                    "type": "object",
                    "description": "e-Invoice details object",
                    "required": [
                      "subtotal",
                      "grandTotal"
                    ],
                    "properties": {
                      "eInvoiceItems": {
                        "type": "array",
                        "description": "List of e-Invoice items",
                        "items": {
                          "properties": {
                            "eInvoiceItemId": {
                              "type": "string",
                              "description": "e-Invoice item ID",
                              "default": "17e95202-d151-43ff-d6af-08db4614f0bb"
                            },
                            "description": {
                              "type": "string",
                              "description": "Item description. If not set in request, it is set to null automatically and the value is returned in response.",
                              "default": "Subscription for TFM tech"
                            },
                            "price": {
                              "type": "number",
                              "description": "Item Price",
                              "default": 10,
                              "format": "double"
                            },
                            "quantity": {
                              "type": "integer",
                              "description": "Quantity of item",
                              "default": 2,
                              "format": "int32"
                            },
                            "itemDiscountType": {
                              "type": "string",
