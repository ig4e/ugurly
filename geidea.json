  "paths": {
    "/api/v1/direct/eInvoice": {
      "post": {
        "summary": "Create Payment Intent",
        "description": "API to create a Payment Intent",
        "operationId": "create-payment-intent",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "amount",
                  "currency",
                  "customer",
                  "eInvoiceDetails"
                ],
                "properties": {
                  "amount": {
                    "type": "number",
                    "description": "Amount for which payment link is created. Amount must be greater than 0.01. Must be equal to [eInvoiceItems.price * eInvoiceItems.quantity]",
                    "default": 20,
                    "format": "double"
                  },
                  "currency": {
                    "type": "string",
                    "description": "Currency for which payment link is created. Currency must be 3 characters in length. Acceptable currencies are: SAR, AED",
                    "default": "SAR",
                    "enum": [
                      "SAR",
                      "EGP",
                      "AED",
                      "QAR",
                      "OMR",
                      "BHD",
                      "KWD",
                      "USD",
                      "GBP",
                      "EUR"
                    ]
                  },
                  "customer": {
                    "type": "object",
                    "description": "Customer Object",
                    "required": [
                      "name",
                      "email",
                      "phoneCountryCode",
                      "phoneNumber"
                    ],
                    "properties": {
                      "name": {
                        "type": "string",
                        "description": "Customer Name",
                        "default": "Kiran"
                      },
                      "email": {
                        "type": "string",
                        "description": "Email of customer. One of email or phoneNumber must be present.",
                        "default": "testmail@geidea.com"
                      },
                      "phoneCountryCode": {
                        "type": "string",
                        "description": "Country code of customer's phone number eg: \"+971\". If phoneNumber is passed, phoneCountryCode must be passed.",
                        "default": "+20"
                      },
                      "phoneNumber": {
                        "type": "string",
                        "description": "Customer's phone number  eg: \"511111111\". If phoneCountryCode is passed, phoneNumber must be passed.  One of email or phoneNumber must be present.",
                        "default": "8003030083"
                      }
                    }
                  },
                  "eInvoiceDetails": {
                    "type": "object",
                    "description": "e-Invoice details object",
                    "required": [
                      "subtotal",
                      "grandTotal",
                      "eInvoiceItems"
                    ],
                    "properties": {
                      "eInvoiceItems": {
                        "type": "array",
                        "description": "List of e-Invoice items",
                        "items": {
                          "properties": {
                            "eInvoiceItemId": {
                              "type": "string",
                              "description": "e-Invoice item ID",
                              "default": "17e95202-d151-43ff-d6af-08db4614f0bb"
                            },
                            "description": {
                              "type": "string",
                              "description": "Item description. If not set in request, it is set to null automatically and the value is returned in response.",
                              "default": "Subscription for TFM tech"
                            },
                            "price": {
                              "type": "number",
                              "description": "Item Price",
                              "default": 10,
                              "format": "double"
                            },
                            "quantity": {
                              "type": "integer",
                              "description": "Quantity of item",
                              "default": 2,
                              "format": "int32"
                            },
                            "itemDiscountType": {
                              "type": "string",
                              "description": "Type of discount applied on item",
                              "default": "Amount",
                              "enum": [
                                "Amount",
                                "Percentage"
                              ]
                            },
                            "taxType": {
                              "type": "string",
                              "description": "Type of tax levied on item",
                              "default": "Amount",
                              "enum": [
                                "Amount",
                                "Percentage"
                              ]
                            },
                            "total": {
                              "type": "number",
                              "description": "Total price including tax levied. Must be equal to [eInvoiceItems.price * eInvoiceItems.quantity]",
                              "default": 20,
                              "format": "double"
                            },
                            "priceWithDiscount": {
                              "type": "number",
                              "description": "Item price after discount is applied. If not set in request, it is set to value of eInvoiceItems.price automatically and the value is returned in response",
                              "format": "double"
                            },
                            "priceTax": {
                              "type": "number",
                              "description": "Tax on Item Price. If not set in request, it is set to zero automatically and the value is returned in response.",
                              "format": "double"
                            },
                            "priceTotal": {
                              "type": "number",
                              "description": "Total Price inclusive of Tax. If not set in request, it is set to value of eInvoiceItems.price automatically and the value is returned in response.",
                              "format": "double"
                            },
                            "sku": {
                              "type": "string",
                              "description": "Item SKU. If not set in request, it is set to null automatically and the value is returned in response."
                            },
                            "itemDiscount": {
                              "type": "number",
                              "description": "Discount applied on item. If not set in request, it is set to null automatically and the value is returned in response.",
                              "format": "double"
                            },
                            "tax": {
                              "type": "number",
                              "description": "Tax on item. If not set in request, it is set to null automatically and the value is returned in response.",
                              "format": "double"
                            },
                            "totalWithoutTax": {
                              "type": "number",
                              "description": "Total price of item without tax. If not set in request, it is set to value of amount automatically and the value is returned in response.",
                              "format": "double"
                            },
                            "totalTax": {
                              "type": "number",
                              "description": "Tax for the item. If not set in request, it is set to zero automatically and the value is returned in response.",
                              "format": "double"
                            }
                          },
                          "required": [
                            "price",
                            "quantity",
                            "total"
                          ],
                          "type": "object"
                        }
                      },
                      "subtotal": {
                        "type": "number",
                        "description": "Subtotal inclusive of tax. Must be equal to [eInvoiceItems.price * eInvoiceItems.quantity]",
                        "default": 20,
                        "format": "double"
                      },
                      "grandTotal": {
                        "type": "number",
                        "description": "Grand Total. Must be equal to [eInvoiceItems.price * eInvoiceItems.quantity]. Must be same as amount.",
                        "default": 20,
                        "format": "double"
                      },
                      "extraChargesType": {
                        "type": "string",
                        "description": "Type of extra charges levied",
                        "default": "Amount",
                        "enum": [
                          "Amount",
                          "Percentage"
                        ]
                      },
                      "invoiceDiscountType": {
                        "type": "string",
                        "description": "Type of discount applied on invoice",
                        "default": "Amount",
                        "enum": [
                          "Amount",
                          "Percentage"
                        ]
                      },
                      "type": {
                        "type": "string",
                        "description": "Type of eInvoice. If not set in request, it is set to \"Detailed\" automatically and the value is returned in response.",
                        "enum": [
                          "Detailed",
                          "Quick"
                        ]
                      },
                      "collectCustomersBillingShippingAddress": {
                        "type": "boolean",
                        "description": "Flag indicating to capture Customer's billing and shipping addresses. If not set in request, it is set to false automatically and the value is returned in response"
                      },
                      "preAuthorizeAmount": {
                        "type": "boolean",
                        "description": "Flag indicating whether to preauthorize the amount. If not set in request, it is set to false automatically and the value is returned in response."
                      },
                      "subtotalWithoutTax": {
                        "type": "number",
                        "description": "Subtotal exclusive of tax. Must be equal to [eInvoiceItems.price * eInvoiceItems.quantity] if passed. If not set in request, it is set to [eInvoiceItems.price * eInvoiceItems.quantity] value automatically and the value is returned in response.",
                        "format": "double"
                      },
                      "subtotalTax": {
                        "type": "number",
                        "description": "Subtotal tax. If not set in request, it is set to zero automatically and the value is returned in response",
                        "format": "double"
                      },
                      "extraCharges": {
                        "type": "number",
                        "description": "Extra charges levied. If not set in request, it is set to null automatically and the value is returned in response.",
                        "format": "double"
                      },
                      "chargeDescription": {
                        "type": "string",
                        "description": "Description of charge levied. If not set in request, it is set to null automatically and the value is returned in response."
                      },
                      "merchantReferenceId": {
                        "type": "string",
                        "description": "The merchant account identifier for which the payment link is created."
                      },
                      "invoiceDiscount": {
                        "type": "number",
                        "description": "Discount applied on invoice. If not set in request, it is set to null automatically and the value is returned in response.",
                        "format": "double"
                      },
                      "language": {
                        "type": "string",
                        "description": "Language to be displayed in Payment Page. If not set in request, it is set to \"EN\" automatically and the value is returned in response.",
                        "enum": [
                          "EN",
                          "AR"
                        ]
                      }
                    }
                  },
                  "expiryDate": {
                    "type": "string",
                    "description": "Expiry date of payment link. If not set in request, it is set to a date of 30 days after activation date automatically and the value is returned in response. If activationDate is not set, the date of 30 days after payment link creation date is set as expiry date and it is returned in response.",
                    "format": "date"
                  },
                  "activationDate": {
                    "type": "string",
                    "description": "Activation date of payment link. If not set in request, it is set to null the value is returned in response.",
                    "format": "date"
                  }
                }
              },
