        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "amount",
                  "currency",
                  "customer"
                ],
                "properties": {
                  "amount": {
                    "type": "number",
                    "description": "Amount for which payment link is created. Amount must be greater than 0.01. Must be equal to [eInvoiceItems.price * eInvoiceItems.quantity]",
                    "default": 100,
                    "format": "double"
                  },
                  "currency": {
                    "type": "string",
                    "description": "Currency for which payment link is created. Currency must be 3 characters in length. Acceptable currencies are: SAR, AED",
                    "default": "SAR",
                    "enum": [
                      "SAR",
                      "EGP",
                      "AED",
                      "QAR",
                      "OMR",
                      "BHD",
                      "KWD",
                      "USD",
                      "GBP",
                      "EUR"
                    ]
                  },
                  "callbackurl": {
                    "type": "string",
                    "description": "endpoint where the details including status of the transaction will be shared"
                  },
                  "merchantReferenceId": {
                    "type": "string"
                  },
                  "activationDate": {
                    "type": "string",
                    "description": "Activation date of payment link. If not set in request, it is set to null the value is returned in response.",
                    "format": "date"
                  },
                  "expiryDate": {
                    "type": "string",
                    "description": "Expiry date of payment link. If not set in request, it is set to a date of 30 days after activation date automatically and the value is returned in response. If activationDate is not set, the date of 30 days after payment link creation date is set as expiry date and it is returned in response.",
                    "format": "date"
                  },
                  "customer": {
                    "type": "object",
                    "description": "Customer Object",
                    "required": [
                      "name",
                      "email",
                      "phoneCountryCode",
                      "phoneNumber"
                    ],
